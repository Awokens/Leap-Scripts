command /toggleitems:
	trigger:
		set {_state} to {randomItems::%player's uuid%} ? true
		if {_state} is true:
			set {_state} to false
		else:
			set {_state} to true
		set {randomItems::%player's uuid%} to {_state}
		send "&7Toggled items to &f%{_state}%"

